<div class="container">
    <div class="row">
      <div class="col-lg-9">
<h2>新規仮登録</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <p><%= f.label :email %></p>
    <p><%= f.email_field :email, autofocus: true, autocomplete: "email" %></p>
    <p><%= f.label :twitter_id %></p>
    <p><%= f.text_field :twitter_id, autofocus: true, autocomplete: "twitter_id" %></p>
  </div>


  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
